t: { name: 'Square Feet', factor: 10.7639 },\r\n                square_inches: { name: 'Square Inches', factor: 1550 },\r\n                acres: { name: 'Acres', factor: 0.000247105 },\r\n                hectares: { name: 'Hectares', factor: 0.0001 }\r\n            },\r\n            time: {\r\n                seconds: { name: 'Seconds', factor: 1 },\r\n                minutes: { name: 'Minutes', factor: 1/60 },\r\n                hours: { name: 'Hours', factor: 1/3600 },\r\n                days: { name: 'Days', factor: 1/86400 },\r\n                weeks: { name: 'Weeks', factor: 1/604800 },\r\n                months: { name: 'Months (30 days)', factor: 1/2592000 },\r\n                years: { name: 'Years (365 days)', factor: 1/31536000 }\r\n            }\r\n        };\r\n\r\n        // DOM elements\r\n        const elements = {\r\n            inputValue: null,\r\n            fromUnit: null,\r\n            toUnit: null,\r\n            resultDisplay: null,\r\n            precision: null,\r\n            scientificNotation: null,\r\n            swapBtn: null,\r\n            copyResult: null,\r\n            favoriteBtn: null\r\n        };\r\n\r\n        // Current category\r\n        let currentCategory = 'length';\r\n\r\n        // Initialize the converter\r\n        document.addEventListener('DOMContentLoaded', function() {\r\n            // Get DOM elements\r\n            elements.inputValue = document.getElementById('inputValue');\r\n            elements.fromUnit = document.getElementById('fromUnit');\r\n            elements.toUnit = document.getElementById('toUnit');\r\n            elements.resultDisplay = document.getElementById('resultDisplay');\r\n            elements.precision = document.getElementById('precision');\r\n            elements.scientificNotation = document.getElementById('scientificNotation');\r\n            elements.swapBtn = document.getElementById('swapBtn');\r\n            elements.copyResult = document.getElementById('copyResult');\r\n            elements.favoriteBtn = document.getElementById('favoriteBtn');\r\n\r\n            // Initialize with length units\r\n            updateUnits('length');\r\n\r\n            // Event listeners\r\n            elements.inputValue.addEventListener('input', convert);\r\n            elements.fromUnit.addEventListener('change', convert);\r\n            elements.toUnit.addEventListener('change', convert);\r\n            elements.precision.addEventListener('change', convert);\r\n            elements.scientificNotation.addEventListener('change', convert);\r\n            elements.swapBtn.addEventListener('click', swapUnits);\r\n            elements.copyResult.addEventListener('click', copyResult);\r\n\r\n            // Category button event listeners\r\n            document.querySelectorAll('.category-btn').forEach(btn => {\r\n                btn.addEventListener('click', function() {\r\n                    const category = this.dataset.category;\r\n                    if (category) {\r\n                        switchCategory(category);\r\n                        this.parentNode.querySelectorAll('.category-btn').forEach(b => b.classList.remove('active'));\r\n                        this.classList.add('active');\r\n                    }\r\n                });\r\n            });\r\n\r\n            // Add intersection observer for animations\r\n            const observerOptions = {\r\n                threshold: 0.1,\r\n                rootMargin: '0px 0px -50px 0px'\r\n            };\r\n\r\n            const observer = new IntersectionObserver((entries) => {\r\n                entries.forEach(entry => {\r\n                    if (entry.isIntersecting) {\r\n                        entry.target.classList.add('fade-in');\r\n                    }\r\n                });\r\n            }, observerOptions);\r\n\r\n            // Observe all feature cards\r\n            document.querySelectorAll('.feature-card').forEach(card => {\r\n                observer.observe(card);\r\n            });\r\n\r\n            // Analytics tracking for CTA clicks\r\n            document.querySelectorAll('button[onclick*=\"initiateUpgrade\"]').forEach(link => {\r\n                link.addEventListener('click', (e) => {\r\n                    if (typeof gtag !== 'undefined') {\r\n                        gtag('event', 'click', {\r\n                            event_category: 'CTA',\r\n                            event_label: 'Upgrade to Pro',\r\n                            value: 1\r\n                        });\r\n                    }\r\n                });\r\n            });\r\n\r\n            document.querySelectorAll('a[href=\"#converter\"]').forEach(link => {\r\n                link.addEventListener('click', (e) => {\r\n                    if (typeof gtag !== 'undefined') {\r\n                        gtag('event', 'click', {\r\n                            event_category: 'CTA',\r\n                            event_label: 'Try Free Now',\r\n                            value: 1\r\n                        });\r\n                    }\r\n                });\r\n            });\r\n        });\r\n\r\n        // Switch category function\r\n        function switchCategory(category) {\r\n            currentCategory = category;\r\n            updateUnits(category);\r\n            convert();\r\n        }\r\n\r\n        // Update unit options based on category\r\n        function updateUnits(category) {\r\n            const units = unitData[category];\r\n            if (!units) return;\r\n\r\n            // Clear existing options\r\n            elements.fromUnit.innerHTML = '';\r\n            elements.toUnit.innerHTML = '';\r\n\r\n            // Add new options\r\n            Object.keys(units).forEach(key => {\r\n                const option1 = new Option(units[key].name, key);\r\n                const option2 = new Option(units[key].name, key);\r\n                elements.fromUnit.appendChild(option1);\r\n                elements.toUnit.appendChild(option2);\r\n            });\r\n\r\n            // Set default selections\r\n            const unitKeys = Object.keys(units);\r\n            if (unitKeys.length > 1) {\r\n                elements.fromUnit.value = unitKeys[0];\r\n                elements.toUnit.value = unitKeys[1];\r\n            }\r\n        }\r\n\r\n        // Convert function\r\n        // Free features showcase function\r\n        function showFreeFeatures() {\r\n            if (typeof gtag !== 'undefined') {\r\n                gtag('event', 'click', {\r\n                    event_category: 'Features',\r\n                    event_label: 'Show Free Features',\r\n                    value: 1\r\n                });\r\n            }\r\n            document.getElementById('features').scrollIntoView({ behavior: 'smooth' });\r\n        }\r\n\r\n        function convert() {\r\n            const value = parseFloat(elements.inputValue.value) || 0;\r\n            const fromUnit = elements.fromUnit.value;\r\n            const toUnit = elements.toUnit.value;\r\n            let precision = parseInt(elements.precision.value) || 2;\r\n            const useScientific = elements.scientificNotation.checked;\r\n\r\n            if (!fromUnit || !toUnit) return;\r\n\r\n            let result = 0;\r\n\r\n            if (currentCategory === 'temperature') {\r\n                result = convertTemperature(value, fromUnit, toUnit);\r\n            } else {\r\n                const units = unitData[currentCategory];\r\n                if (units && units[fromUnit] && units[toUnit]) {\r\n                    const fromFactor = units[fromUnit].factor;\r\n                    const toFactor = units[toUnit].factor;\r\n                    result = (value / fromFactor) * toFactor;\r\n                }\r\n            }\r\n\r\n            // All precision levels are now FREE\r\n            // No limitations - use selected precision\r\n\r\n            // Format result\r\n            let formattedResult = '';\r\n            if (useScientific && Math.abs(result) >= 1000) {\r\n                formattedResult = result.toExponential(precision);\r\n            } else {\r\n                formattedResult = result.toFixed(precision);\r\n            }\r\n\r\n            elements.resultDisplay.textContent = formattedResult;\r\n        }\r\n\r\n        // Temperature conversion function\r\n        function convertTemperature(value, fromUnit, toUnit) {\r\n            if (fromUnit === toUnit) return value;\r\n\r\n            // Convert to Celsius first\r\n            let celsius = value;\r\n            if (fromUnit === 'fahrenheit') {\r\n                celsius = (value - 32) * 5/9;\r\n            } else if (fromUnit === 'kelvin') {\r\n                celsius = value - 273.15;\r\n            }\r\n\r\n            // Convert from Celsius to target\r\n            if (toUnit === 'fahrenheit') {\r\n                return celsius * 9/5 + 32;\r\n            } else if (toUnit === 'kelvin') {\r\n                return celsius + 273.15;\r\n            }\r\n\r\n            return celsius;\r\n        }\r\n\r\n        // Swap units function\r\n        function swapUnits() {\r\n            const fromValue = elements.fromUnit.value;\r\n            const toValue = elements.toUnit.value;\r\n            \r\n            elements.fromUnit.value = toValue;\r\n            elements.toUnit.value = fromValue;\r\n            \r\n            convert();\r\n        }\r\n\r\n        // Copy result function\r\n        function copyResult() {\r\n            const result = elements.resultDisplay.textContent;\r\n            navigator.clipboard.writeText(result).then(() => {\r\n                // Show feedback\r\n                const originalText = elements.copyResult.textContent;\r\n                elements.copyResult.textContent = '‚úì Copied!';\r\n                setTimeout(() => {\r\n                    elements.copyResult.textContent = originalText;\r\n                }, 2000);\r\n            });\r\n        }\r\n\r\n        // Pro features modal functions\r\n        function showProFeatures() {\r\n            document.getElementById('proFeaturesModal').style.display = 'block';\r\n        }\r\n\r\n        function closeProFeatures() {\r\n            document.getElementById('proFeaturesModal').style.display = 'none';\r\n        }\r\n\r\n        function initiateUpgrade() {\r\n            // Track upgrade intent\r\n            if (typeof gtag !== 'undefined') {\r\n                gtag('event', 'click', {\r\n                    event_category: 'Upgrade',\r\n                    event_label: 'Initiate Upgrade',\r\n                    value: 1\r\n                });\r\n            }\r\n            \r\n            // Show upgrade modal or redirect to payment\r\n            alert('Upgrade feature coming soon! All features are currently FREE.');\r\n        }\r\n\r\n        // Close modal when clicking outside\r\n        window.onclick = function(event) {\r\n            const modal = document.getElementById('proFeaturesModal');\r\n            if (event.target === modal) {\r\n                modal.style.display = 'none';\r\n            }\r\n        }\r\n    </script>\r\n</body>\r\n</html>